osmium tags-filter latest.osm.pbf  r/type=boundary r/boundary=administrative  -o admin.osm.pbf

osmium export admin.osm.pbf  --geometry-types=polygon  -a id,type  -o admin.geojson

jq '
  .features |= map(
    select(
      (.properties.admin_level == "2" or
       .properties.admin_level == "4" or
       .properties.admin_level == "8") and
      (.geometry.type == "Polygon" or .geometry.type == "MultiPolygon") and
      (.properties["@type"] == "relation")
    )
  )
' admin.geojson > admin-248.geojson


jq -r '.features[].properties.admin_level' admin-248.geojson | sort | uniq -c

